def insertion_sort(arr):
    # Проходим по элементам массива начиная со второго (с индексом 1)
    for i in range(1, len(arr)):
        # Запоминаем текущий элемент, который нужно вставить в отсортированную часть
        key = arr[i]
        
        # Инициализируем j как индекс предыдущего элемента
        j = i - 1
        
        # Сдвигаем элементы массива, которые больше key, вправо, чтобы освободить место для вставки key
        while j >= 0 and key < arr[j]:
            arr[j + 1] = arr[j]
            j -= 1
        
        # Вставляем key на правильное место в отсортированной части массива
        arr[j + 1] = key
    
    # Возвращаем отсортированный массив
    return arr


# Пример использования функции
numbers = [12, 11, 13, 5, 6]
print(insertion_sort(numbers))  # Выводим отсортированный массив
